E_000001f230c2ca10 .event posedge, v000001f230c02e10_0;
S_000001f230c33680 .scope task, "read_memory" "read_memory" 2 61, 2 61 0, S_000001f230d8ad80;
 .timescale 0 0;
v000001f230c33810_0 .var "num_loc", 4 0;
v000001f230c338b0_0 .var "start_loc", 3 0;
E_000001f230c2c4d0 .event anyedge, v000001f230d8eb80_0;
TD_tb.read_memory ;
    %load/vec4 v000001f230c338b0_0;
    %pad/u 32;
    %store/vec4 v000001f230c34c10_0, 0, 32;
T_0.0 ;
    %load/vec4 v000001f230c34c10_0;
    %load/vec4 v000001f230c338b0_0;
    %pad/u 32;
    %load/vec4 v000001f230c33810_0;
    %pad/u 32;
    %add;
    %cmp/u;
    %jmp/0xz T_0.1, 5;
    %wait E_000001f230c2ca10;
    %load/vec4 v000001f230c34c10_0;
    %pad/s 4;
    %store/vec4 v000001f230c33c20_0, 0, 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001f230c34670_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001f230c33d10_0, 0, 1;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v000001f230c34c10_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v000001f230c34c10_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
T_0.2 ;
    %load/vec4 v000001f230c34a30_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_0.3, 6;
    %wait E_000001f230c2c4d0;
    %jmp T_0.2;
T_0.3 ;
    %wait E_000001f230c2ca10;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001f230c33d10_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001f230c34670_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v000001f230c33c20_0, 0, 4;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v000001f230c34530_0, 0, 16;
    %end;
S_000001f230c33950 .scope task, "write_memory" "write_memory" 2 43, 2 43 0, S_000001f230d8ad80;
 .timescale 0 0;
v000001f230c33ae0_0 .var "num_loc", 4 0;
v000001f230c33b80_0 .var "start_loc", 3 0;
TD_tb.write_memory ;
    %load/vec4 v000001f230c33b80_0;
    %pad/u 32;
    %store/vec4 v000001f230c34c10_0, 0, 32;
T_1.4 ;
    %load/vec4 v000001f230c34c10_0;
    %load/vec4 v000001f230c33b80_0;
    %pad/u 32;
    %load/vec4 v000001f230c33ae0_0;
    %pad/u 32;
    %add;
    %cmp/u;
    %jmp/0xz T_1.5, 5;
    %wait E_000001f230c2ca10;
    %load/vec4 v000001f230c34c10_0;
    %pad/s 4;
    %store/vec4 v000001f230c33c20_0, 0, 4;
    %vpi_func 2 48 "$random" 32 {0 0 0};
    %pad/s 16;
    %store/vec4 v000001f230c34530_0, 0, 16;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001f230c34670_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001f230c33d10_0, 0, 1;
    %load/vec4 v000001f230c34c10_0;
    %addi 1, 0, 32;
    %store/vec4 v000001f230c34c10_0, 0, 32;
    %jmp T_1.4;
T_1.5 ;
T_1.6 ;
    %load/vec4 v000001f230c34a30_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %cmpi/ne 1, 0, 1;
    %jmp/0xz T_1.7, 6;
    %wait E_000001f230c2c4d0;
    %jmp T_1.6;
T_1.7 ;
    %wait E_000001f230c2ca10;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001f230c33d10_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001f230c34670_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v000001f230c33c20_0, 0, 4;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v000001f230c34530_0, 0, 16;
    %end;
    .scope S_000001f230d8e8b0;
T_2 ;
    %wait E_000001f230c2ca10;
    %load/vec4 v000001f230d8ec20_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_2.0, 4;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v000001f230d8eae0_0, 0, 16;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001f230d8eb80_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v000001f230c2aa60_0, 0, 32;
T_2.2 ;
    %load/vec4 v000001f230c2aa60_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_2.3, 5;
    %pushi/vec4 0, 0, 16;
    %ix/getv/s 4, v000001f230c2aa60_0;
    %store/vec4a v000001f230d8ea40, 4, 0;
    %load/vec4 v000001f230c2aa60_0;
    %addi 1, 0, 32;
    %store/vec4 v000001f230c2aa60_0, 0, 32;
    %jmp T_2.2;
T_2.3 ;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v000001f230d8ecc0_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_2.4, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001f230d8eb80_0, 0, 1;
    %load/vec4 v000001f230c335e0_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_2.6, 4;
    %load/vec4 v000001f230c33540_0;
    %load/vec4 v000001f230c28ff0_0;
    %pad/u 6;
    %ix/vec4 4;
    %store/vec4a v000001f230d8ea40, 4, 0;
    %jmp T_2.7;
T_2.6 ;
    %load/vec4 v000001f230c28ff0_0;
    %pad/u 6;
    %ix/vec4 4;
    %load/vec4a v000001f230d8ea40, 4;
    %store/vec4 v000001f230d8eae0_0, 0, 16;
T_2.7 ;
    %jmp T_2.5;
T_2.4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001f230d8eb80_0, 0, 1;
T_2.5 ;
T_2.1 ;
    %jmp T_2;
    .thread T_2;
    .scope S_000001f230d8ad80;
T_3 ;
    %delay 5, 0;
    %load/vec4 v000001f230c345d0_0;
    %inv;
    %store/vec4 v000001f230c345d0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_000001f230d8ad80;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001f230c345d0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001f230c34490_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001f230c34490_0, 0, 1;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v000001f230c33b80_0, 0, 4;
    %pushi/vec4 16, 0, 5;
    %store/vec4 v000001f230c33ae0_0, 0, 5;
    %fork TD_tb.write_memory, S_000001f230c33950;
    %join;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v000001f230c338b0_0, 0, 4;
    %pushi/vec4 16, 0, 5;
    %store/vec4 v000001f230c33810_0, 0, 5;
    %fork TD_tb.read_memory, S_000001f230c33680;
    %join;
    %delay 100, 0;
    %vpi_call 2 39 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_000001f230d8ad80;
T_5 ;
    %vpi_call 2 80 "$dumpvars" {0 0 0};
    %vpi_call 2 81 "$dumpfile", "waves.vcd" {0 0 0};
    %end;
    .thread T_5;
